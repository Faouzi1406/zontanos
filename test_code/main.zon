fn print(value: i8^) string[13] {
	printf(^"value: %s", ^value)
	return  "Hello, world!"
}

fn readChar() i8 {	
	let value: char = getchar();
	return value
}

fn putChar(charToPut: char) char {
	putchar(charToPut)
	return charToPut	
}

fn main() i32 {
	printf(^"%c", readChar())
	putChar('a')
	return 0;
}
