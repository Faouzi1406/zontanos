fn println(value: i8^) i32 {
	let chars: i32 = printf(^"%s%c", ^value, '\n')
	return chars
}

fn readChar() i8 {	
	let value: char = getchar();
	return value
}

fn putChar(charToPut: char) char {
	putchar(charToPut)
	return charToPut	
}

fn welkom() void {
	println(^"Welkom my friend")
	return void
}

fn main() i32 {
	let some: i32 = 10
	some = 20
	printf(^"%d%c", some, '\n')
	welkom()
	return 0;
}
