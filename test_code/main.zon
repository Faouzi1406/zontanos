fn println(value: i8^) i32 {
	let chars: i32 = printf(^"%s%c", ^value, '\n')
	return chars
}

fn readChar() i8 {	
	let value: char = getchar();
	return value
}

fn putChar(charToPut: char) char {
	putchar(charToPut)
	return charToPut	
}

fn welkom() void {
	println(^"Welkom my friend")
	return void
}

fn recurse(value: i32, range: i32) i32 {
	printf(^"%d%c", value,'\n')
	if range >= value  {
		return value;
	} else {
		printf(^"%d%c", value,'\n')

		let v: i32 = value;
		v += 1;
	 	return recurse(v, range);
	}

	return 18
}

fn main() void {
	recurse(0, 200)
	if 10 > 20 {
		printf(^"10 is more then 20");
		return void;
	} else {
		printf(^"10 is not more then 20");
		return void;
	}
}
