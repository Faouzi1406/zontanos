fn println(value: i8^) string[13] {
	printf(^"%s%c", ^value, '\n')
	return  "Hello, world!"
}

fn readChar() i8 {	
	let value: char = getchar();
	return value
}

fn putChar(charToPut: char) char {
	putchar(charToPut)
	return charToPut	
}

fn welkom() void {
	println(^"Welkom my friend")
	return void
}

fn main() i32 {
	welkom()
	return 0;
}
