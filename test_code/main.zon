fn println(value: i8^) i32 {
	let chars: i32 = printf(^"%s%c", ^value, '\n')
	return chars
}

fn readChar() i8 {	
	let value: char = getchar();
	return value
}

fn putChar(charToPut: char) char {
	putchar(charToPut)
	return charToPut	
}

fn welkom() void {
	println(^"Welkom my friend")
	return void
}

fn recurse(value: i32, limit: i32) i32 {
	printf(^"beepboop %d%c", value, '\n')

	if value >= limit {
		return 18;
	} else {
		let v: i32 = value;
		v += 1;
	 	return recurse(v, limit);
	}
	return 18
}

fn main() void {
	let fun: i32 = recurse(0, 200)
	if fun > 20 || 9 > 10 {
		printf(^"whut")
		return void
	} else {
		printf(^"nope")
		return void
	}
	return void
}
